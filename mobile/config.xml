<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.racheh.socialApp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>social-app</name>
  <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
  <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
  <content src="index.html"/>
  <access origin="*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <platform name="android">
    <allow-intent href="market:*"/>
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
  </platform>
  <plugin name="cordova-plugin-whitelist" spec="1"/>
  <plugin name="cordova-plugin-customconfigparameters" spec="~3.0.0"/>
  <plugin name="cordova-plugin-sms" spec="~1.0.5"/>
  <plugin name="cordova-plugin-contacts-phonenumbers" spec="~0.0.12"/>
  <plugin name="cordova-plugin-android-permissions" spec="^1.0.0"/>
  <preference name="runtime" value="https://-.cloudfunctions.net/api"/>
</widget>